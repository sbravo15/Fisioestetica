@model IEnumerable<G7.Models.ExpedientesObj>

@{
    ViewBag.Title = "VerExpediente";
}

<h2>VerExpediente</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <tr>
        <th>
            Número de expediente
        </th>
        <th>
            Id del Paciente
        </th>
        <th>
            Id del Doctor
        </th>
        <th>
            Padecimiento
        </th>
        <th>
            Tratamiento
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdExpediente)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUsuario)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Padecimiento)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tratamiento)
            </td>
           
        </tr>
    }

</table>
