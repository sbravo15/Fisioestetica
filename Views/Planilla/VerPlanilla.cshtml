@model IEnumerable<G7.Models.PlanillaObj>

@{
    ViewBag.Title = "VerPlanilla";
}

<h2>VerPlanilla</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
    <tr>
        <th>
            Id planilla
        </th>
        <th>
            Id Doctor
        </th>
        <th>
            Horas trabajadas al mes
        </th>
        <th>
            Salario bruto
        </th>
        <th>
            Seguro
        </th>
        <th>
            Deducciones
        </th>
        <th>
            Salario neto
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.IdPlanilla)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDoctor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HorasTrabajadas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SalarioBruto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Seguro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Deducciones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SalarioNeto)
            </td>
            
        </tr>
    }

</table>
